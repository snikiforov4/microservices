{
    "cniVersion": "0.3.1",
    "name": "bridge",
    "type": "bridge",
    "bridge": "cnio0",
    "isGateway": true,
    "ipMasq": true,
    "ipam": {
        "type": "host-local",
        "ranges": [
          [{"subnet": "{{ hostvars[inventory_hostname].gce_metadata['pod-cidr'] }}"]
        ],
        "routes": [{"dst": "0.0.0.0/0"}]
    }
}
